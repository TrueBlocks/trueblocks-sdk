{
  "name": "@trueblocks/sdk",
  "homepage": "https://github.com/TrueBlocks/trueblocks-sdk",
  "repository": "TrueBlocks:trueblocks-sdk",
  "version": "0.15.12",
  "license": "GPL-3.0-or-later",
  "description": "TypeScript SDK for TrueBlocks Chifra",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./README.md": "./README.md"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest",
    "lint": "eslint --fix . --ext js,cjs,mjs,ts --cache",
    "format": "prettier . --ignore-path .eslintignore --write --ignore-unknown",
    "prerelease": "yarn install && yarn build",
    "release": "bumpp package.json --commit --push --tag",
    "preinstall": "node --eval \"if(process.env.npm_execpath.indexOf('yarn')===-1) throw new Error('You must use Yarn to install, not NPM')\"",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.14.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "packageManager": "yarn@1.22.19",
  "publishConfig": {
    "access": "public",
    "github-package-registry": "https://npm.pkg.github.com"
  }
}